    "keywords": ["yii2", "blade", "renderer"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "xyb",
            "email": "781028081@qq.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": { "xyb\\blade\\": "" }
